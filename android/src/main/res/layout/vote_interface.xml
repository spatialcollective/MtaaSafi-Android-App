<?xml version="1.0" encoding="utf-8"?>

<com.sc.mtaasafi.android.feed.VoteInterface xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="8dp"
    android:paddingTop="4dp"
    android:gravity="center_horizontal">
    <ImageButton
        android:layout_width="51dp"
        android:layout_height="51dp"
        android:id="@+id/upvoteButton"
        android:src="@drawable/button_upvote_unclicked"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:focusable="false"
        android:focusableInTouchMode="false"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="200"
        android:id="@+id/upvoteCount" />
</com.sc.mtaasafi.android.feed.VoteInterface>

